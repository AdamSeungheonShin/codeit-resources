{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@ui/*": [
        "../../packages/ui/*"
      ],
      "@/public/*": [
        "./public/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "nativewind-env.d.ts",
    "**/__tests__/**/*.ts",
    "**/__tests__/**/*.tsx",
    "**/*.d.ts",
    "expo-env.d.ts"
  ]
}
