branch_name=$(git branch --show-current)

if [[ "$branch_name" =~ ^[0-9]+-fe$ ]]; then
  default_message="FE-"
elif [[ "$branch_name" =~ ^[0-9]+-be$ ]]; then
  default_message="BE-"
else
  default_message=""
fi

commit_message=$(cat "$1" | xargs)

if [ -n "$default_message" ]; then
  message=$(cat "$1" | xargs)
  
  echo "$default_message$message" > "$1"
fi